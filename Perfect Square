#include<stdio.h>
int main()
 {
     int x;
 printf("enter the perfect square");
 scanf("%d",&x);
 calculate (x);
return 0;
 }
 void calculate(int x)
 {
     int i,square=0;
     for(i=1;i<x;i++)
     {
         if (x%i==0)
         {
             square=square+i;
         }
     }
     if(square==x)
     printf("%d is perfect square",x);
     else
     printf("%d is not a perfect square",x);
 }
